<test title="Drop a function">
<before>
    <schema>
        <function name="sales_tax" arguments="subtotal float" returns="float">
BEGIN
    RETURN subtotal * 0.06;
END;
        </function>
    </schema>
</before>
<after>
    <schema>
    </schema>
</after>
<ddls dbms="postgres">
    <ddl>DROP FUNCTION sales_tax(float)</ddl>
</ddls>
<ddls dbms="mysql">
    <ddl>DROP FUNCTION sales_tax</ddl>
</ddls>
<ddls dbms="firebird">
    <ddl>DROP PROCEDURE sales_tax</ddl>
</ddls>
</test>
