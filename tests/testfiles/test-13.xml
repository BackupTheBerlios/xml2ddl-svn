<test title="Create two indexes">
<before>
    <schema>
    <table name="table1">
        <columns>
            <column name="col1" type="integer"/>
            <column name="col2" type="integer"/>
        </columns>
    </table>
    </schema>
</before>
<after>
    <schema>
    <table name="table1">
        <columns>
            <column name="col1" type="integer"/>
            <column name="col2" type="integer"/>
        </columns>
        <indexes>
            <index columns="col1"/>
            <index name="col1col2" columns="col1,col2"/>
        </indexes>
    </table>
    </schema>
</after>
<ddls>
    <ddl>CREATE INDEX table1Col1 ON table1 (col1)</ddl>
    <ddl>CREATE INDEX col1col2 ON table1 (col1, col2)</ddl>
</ddls>
</test>
